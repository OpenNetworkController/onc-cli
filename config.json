{
  "devices": [
    {
      "enabled": false,
      "deviceId": "ubnt,edgerouter-x",
      "version": "21.02.3",
      "roles": ["router"],
      "system": {
        "hostname": "jaculu-router"
      }
    },
    {
      "deviceId": "tplink,archer-c50-v4",
      "version": "21.02.2",
      "roles": ["ap"],
      "untagged_vlan_ip": "10.0.0.219",
      "provisioning_config": {
        "ssh_auth": {
          "username": "root",
          "password": "123123"
        }
      },
      "system": {
        "hostname": "jaculu-ap-2"
      }
    }
  ],
  "config": {
    "system": {
      "timezone": "Africa/Johannesburg"
    },
    "firewall": {
      "defaults": {
        "input": "ACCEPT",
        "output": "ACCEPT",
        "forward": "REJECT",
        "synflood_protect": true
      },
      "zones": [
        {
          "name": "wan",
          "input": "REJECT",
          "output": "ACCEPT",
          "forward": "REJECT",
          "masq": true,
          "mtu_fix": true,
          "network": ["wan"]
        },
        {
          "name": "lan",
          "input": "ACCEPT",
          "output": "ACCEPT",
          "forward": "ACCEPT",
          "network": ["lan"]
        },
        {
          "name": "guest",
          "input": "REJECT",
          "output": "ACCEPT",
          "forward": "REJECT",
          "network": ["guest"]
        }
      ],
      "forwardings": [
        {
          "src": "lan",
          "dest": "wan"
        },
        {
          "src": "guest",
          "dest": "wan"
        },
        {
          "src": "lan",
          "dest": "guest"
        }
      ],
      "rules": [
        {
          "name": "Allow-DHCP-Renew",
          "proto": ["udp"],
          "family": "ipv4",
          "src": "wan",
          "dest_port": [68],
          "target": "ACCEPT"
        },
        {
          "name": "Allow-Ping",
          "src": "wan",
          "proto": ["icmp"],
          "icmp_type": ["echo-request"],
          "family": "ipv4",
          "target": "ACCEPT"
        },
        {
          "name": "Allow-IGMP",
          "src": "wan",
          "family": "ipv4",
          "proto": ["igmp"],
          "target": "ACCEPT"
        },
        {
          "name": "Allow-DHCPv6",
          "src": "wan",
          "proto": ["udp"],
          "family": "ipv6",
          "src_ip": ["fc00::/6"],
          "dest_ip": ["fc00::/6"],
          "dest_port": [546],
          "target": "ACCEPT"
        },
        {
          "name": "Allow-MLD",
          "proto": ["icmp"],
          "family": "ipv6",
          "src": "wan",
          "src_ip": ["fe80::/10"],
          "icmp_type": ["130/0", "131/0", "132/0", "143/0"],
          "target": "ACCEPT"
        },
        {
          "name": "Allow-ICMPv6-Input",
          "proto": ["icmp"],
          "icmp_type": [
            "echo-request",
            "echo-reply",
            "destination-unreachable",
            "packet-too-big",
            "time-exceeded",
            "bad-header",
            "unknown-header-type",
            "router-solicitation",
            "neighbour-solicitation",
            "router-advertisement",
            "neighbour-advertisement"
          ],
          "family": "ipv6",
          "src": "wan",
          "limit": "1000/sec",
          "target": "ACCEPT"
        },
        {
          "name": "Allow-ICMPv6-Forward",
          "src": "wan",
          "dest": "*",
          "proto": ["icmp"],
          "icmp_type": [
            "echo-request",
            "echo-reply",
            "destination-unreachable",
            "packet-too-big",
            "time-exceeded",
            "bad-header",
            "unknown-header-type"
          ],
          "limit": "1000/sec",
          "family": "ipv6",
          "target": "ACCEPT"
        },
        {
          "name": "Allow-IPSec-ESP",
          "src": "wan",
          "dest": "lan",
          "proto": ["esp"],
          "target": "ACCEPT"
        },
        {
          "name": "Allow-ISAKMP",
          "src": "wan",
          "dest": "lan",
          "dest_port": [500],
          "proto": ["udp"],
          "target": "ACCEPT"
        },
        {
          "name": "Support-UDP-Traceroute",
          "src": "wan",
          "dest_port": "33434:33689",
          "proto": ["udp"],
          "family": "ipv4",
          "target": "REJECT",
          "enabled": false
        },
        {
          "name": "Guest DHCP and DNS",
          "src": "guest",
          "dest_port": [53, 67, 68],
          "target": "ACCEPT"
        }
      ]
    },
    "dhcp": {
      "dnsmasq": {
        "domainneeded": true,
        "localise_queries": true,
        "rebind_localhost": true,
        "local": "/lan/",
        "expandhosts": true,
        "authoritative": true,
        "readethers": true,
        "leasefile": "/tmp/dhcp.leases",
        "localservice": true,
        "ednspacket_max": 1232,
        "dnsforwardmax": 300,
        "domain": "lan",
        "noresolv": true
      }
    },
    "network": {
      "networks": [
        {
          "name": "wan",
          "router": {
            "device": "eth0",
            "proto": "pppoe"
          }
        },
        {
          "name": "lan",
          "vlan": 1,
          "vlan_untagged": true,
          "router": {
            "proto": "static",
            "ipaddr": "10.0.0.1",
            "netmask": "255.255.0.0"
          },
          "non_router": {
            "proto": "dhcp"
          }
        },
        {
          "name": "guest",
          "vlan": 2,
          "router": {
            "proto": "static",
            "ipaddr": "10.1.0.1",
            "netmask": "255.255.0.0"
          },
          "non_router": {
            "proto": "dhcp"
          }
        }
      ]
    },
    "wireless": {
      "wifi-iface": [
        {
          "mode": "ap",
          "ssid": "Test yolo",
          "network": "lan",
          "key": "test123456",
          "encryption": "psk2"
        }
      ]
    }
  }
}
