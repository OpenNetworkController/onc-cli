{
  "target": {
    "deviceId": "tplink,archer-c50-v4",
    "openWrtVersion": "22.01.3"
  },
  "system": {
    "hostname": "test",
    "timezone": "Africa/Johannesburg"
  },
  "dhcp": {
    "dnsmasq": [
      {
        "domainneeded": true,
        "boguspriv": true,
        "filterwin2k": false,
        "localise_queries": true,
        "rebind_protection": true,
        "rebind_localhost": true,
        "local": "/lan/",
        "domain": "lan",
        "expandhosts": true,
        "nonegcache": false,
        "authoritative": true,
        "readethers": true,
        "leasefile": "/tmp/dhcp.leases",
        "resolvfile": "/tmp/resolv.conf.d/resolv.conf.auto",
        "nonwildcard": true,
        "localservice": true,
        "ednspacket_max": 1232
      }
    ]
  },
  "network": {
    "devices": [
      {
        "name": "br-lan",
        "type": "bridge",
        "ports": ["eth0", "eth1", "eth2", "eth3", "eth4"],
        "vlans": [
          {
            "id": 1,
            "ports": [
              { "name": "eth0", "status": "untagged" },
              { "name": "eth1", "status": "untagged" },
              { "name": "eth2", "status": "untagged" },
              { "name": "eth3", "status": "untagged" },
              { "name": "eth4", "status": "untagged" }
            ]
          }
        ]
      }
    ],
    "interfaces": [
      {
        "name": "lan",
        "device": "br-lan.1",
        "proto": "dhcp"
      }
    ]
  },
  "wireless": {
    "radios": [
      {
        "name": "radio0",
        "channel": 4
      },
      {
        "name": "radio1",
        "channel": 36
      }
    ],
    "networks": [
      {
        "device": "radio0",
        "mode": "ap",
        "ssid": "Test",
        "encryption": "sae",
        "key": "123123123",
        "network": "lan"
      },
      {
        "device": "radio1",
        "mode": "ap",
        "ssid": "Test",
        "encryption": "sae",
        "key": "123123123",
        "network": "lan"
      }
    ]
  }
}
